@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix essdivesamplerf: <https://w3id.org/heycatwonton/EssDiveSampleRF/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

essdivesamplerf:EssDiveSampleCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "EssDiveSampleCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom essdivesamplerf:EssDiveSample ;
            owl:onProperty essdivesamplerf:entries ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:entries ] ;
    skos:definition "A holder for EssDiveSample objects" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:EssDiveSample a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "EssDiveSample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:birth_date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:age_in_years ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty essdivesamplerf:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom essdivesamplerf:PersonStatus ;
            owl:onProperty essdivesamplerf:vital_status ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty essdivesamplerf:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty essdivesamplerf:birth_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:age_in_years ],
        essdivesamplerf:NamedThing ;
    skos:definition "Represents a EssDiveSample" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty essdivesamplerf:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty essdivesamplerf:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty essdivesamplerf:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty essdivesamplerf:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty essdivesamplerf:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty essdivesamplerf:name ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

PATO:0001421 a owl:Class,
        essdivesamplerf:PersonStatus ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf essdivesamplerf:PersonStatus .

PATO:0001422 a owl:Class,
        essdivesamplerf:PersonStatus ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf essdivesamplerf:PersonStatus .

<https://w3id.org/heycatwonton/EssDiveSampleRF/PersonStatus#UNKNOWN> a owl:Class,
        essdivesamplerf:PersonStatus ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf essdivesamplerf:PersonStatus .

essdivesamplerf:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:birth_date a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:primary_email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range essdivesamplerf:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/heycatwonton/EssDiveSampleRF> .

essdivesamplerf:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/heycatwonton/EssDiveSampleRF/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/heycatwonton/EssDiveSampleRF/PersonStatus#UNKNOWN> .

<https://w3id.org/heycatwonton/EssDiveSampleRF> a owl:Ontology ;
    rdfs:label "EssDiveSampleRF" ;
    dcterms:license "MIT" ;
    dcterms:title "EssDiveSampleRF" ;
    rdfs:seeAlso <https://heycatwonton.github.io/EssDiveSampleRF> ;
    skos:definition "ESS-Dive Sample RF" .

